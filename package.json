{
  "name": "eslint-plugin-feature-sliced-design-imports",
  "version": "1.1.1",
  "description": "A custom ESLint plugin designed to enforce strict import rules in projects following the Feature-Sliced Design architecture.",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "feature-sliced-design",
    "feature-sliced-design-imports",
    "fsd",
    "fsd-plugin",
    "feature-sliced",
    "eslint-plugin-fsd"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/DevsHorse/eslint-plugin-feature-sliced-design-imports.git"
  },
  "bugs": {
    "url": "https://github.com/DevsHorse/eslint-plugin-feature-sliced-design-imports/issues"
  },
  "homepage": "https://github.com/DevsHorse/eslint-plugin-feature-sliced-design-imports#readme",
  "author": "devhorse",
  "main": "./lib/index.js",
  "exports": "./lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "npm-run-all \"lint:*\"",
